<h1>Adding a new post!</h1>

<%= form_for :post, url: posts_path do |f| %>
    <!-- TODO: add css to make div behave like table -->
    <div>
        <p> <%= f.label :title %>: <br>
            <%= f.text_field :title %> </p>
        <p> <%= f.label :body  %>: <br>
            <%= f.text_area :body %> </p> 
    </div>
    <div>
        <p><%= f.submit %></p>
    </div>
<% end %>